<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dbltecnologia.pedmoto" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pedmoto</name>
    <description>O seu aplicativo de corridas</description>
    <author email="admin@dbltecnologia.com.br" href="https://www.pedmoto.com.br">Pedmoto</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="about:blank" />
    <allow-navigation href="https://api.pedmoto.com.br/*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="android-minSdkVersion" value="28" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
</widget>
